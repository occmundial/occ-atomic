{
  "name": "@occmundial/occ-atomic",
  "version": "1.9.5",
  "description": "Collection of shareable styled React components for OCC applications.",
  "homepage": "http://occmundial.github.io/occ-atomic",
  "main": "build/index.js",
  "scripts": {
    "prestart": "npm run gen-docs",
    "start": "set NODE_ENV=development && npm-run-all --parallel start-docs gen-docs:watch",
    "start-docs": "node scripts/server.js",
    "gen-docs": "node scripts/generateComponentData.js",
    "gen-docs:watch": "npm run gen-docs -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "babel lib -d build",
    "build:watch": "babel lib -w -d build",
    "build:bundle": "babel-node scripts/build.js",
    "lint": "esw webpack.config.* lib src --color",
    "lint:watch": "npm run lint -- --watch",
    "prepare": "npm run build",
    "predeploy": "set NODE_ENV=production && npm run build:bundle && npm run playroom:build",
    "deploy": "gh-pages -d dist",
    "playroom:start": "playroom start",
    "playroom:build": "playroom build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/occmundial/occ-atomic"
  },
  "author": "Luis DÃ­az",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "base64-url": "^2.2.2",
    "chokidar": "^2.0.3",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "gh-pages": "^1.2.0",
    "highlight.js": "^9.12.0",
    "jest": "^22.4.3",
    "jsdom": "^11.11.0",
    "npm-run-all": "^4.1.3",
    "playroom": "^0.9.1",
    "prop-types": "^15.6.0",
    "react": "^16.4.1",
    "react-addons-test-utils": "^15.6.2",
    "react-docgen": "^2.20.1",
    "react-dom": "^16.4.1",
    "react-meta-tags": "^0.7.4",
    "react-test-renderer": "^16.3.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.0.0",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "class-autobind": "^0.1.4",
    "hex-rgba": "^1.0.2",
    "idx": "^2.5.6",
    "jquery": "^3.2.1",
    "lodash": "^4.17.14",
    "natives": "1.1.6",
    "react-jss": "8.6.0",
    "react-text-mask": "^5.4.3",
    "react-waypoint": "^8.0.0"
  },
  "jest": {
    "testURL": "https://occmundial.github.io/occ-atomic",
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "roots": [
      "<rootDir>/lib/"
    ]
  }
}
